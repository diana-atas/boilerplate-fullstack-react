{
  "name": "boilerplate-fullstack-react",
  "version": "0.0.1",
  "description": "see readme",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diana-atas/boilerplate-fullstack-react.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "fullstack"
  ],
  "author": "d-atas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diana-atas/boilerplate-fullstack-react/issues"
  },
  "homepage": "https://github.com/diana-atas/boilerplate-fullstack-react#readme",
  "scripts": {
    "start": "node server",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon --watch server server/index.js",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx .",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "sqlite3": "^5.0.8",
    "express": "^4.17.2",
    "knex": "^2.1.0",
    "pg": "^8.7.1",
    "superagent": "7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.3",
    "babel-loader": "^8.2.2",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "supertest": "^6.2.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
